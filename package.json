{
  "name": "@allcapsdev/cvcsv-cli",
  "version": "2.2.115",
  "description": "CLI for creating CSV for Cloud Vision Product Search API",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "cvcsv": "bin/index.js"
  },
  "files": [
    "lib/**/*",
    "bin/**/*"
  ],
  "scripts": {
    "build": "npm run clean:some && rollup -c",
    "clean:lib": "rimraf ./lib",
    "clean:docs": "rimraf ./docs",
    "clean:node_modules": "rimraf ./node_modules",
    "clean:some": "run-p clean:lib clean:docs",
    "clean": "run-p prettier clean:lib clean:docs clean:node_modules",
    "commit": "git-cz",
    "commitlint": "commitlint -e ./.git/COMMIT_EDITMSG",
    "release": "standard-version",
    "start:dev": "ts-node -T src/index.ts",
    "test": "jest",
    "watch": "jest --watch",
    "typedoc": "typedoc --out docs ./src",
    "prepare": "npm run build",
    "prettier": "prettier --parser typescript --write \"src/**/*.ts\"",
    "pretty-quick": "pretty-quick",
    "size": "size-limit",
    "get-version": "echo $npm_package_version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allcapsdev/cvcsv-cli.git"
  },
  "keywords": [
    "Google Cloud Vision Product Search",
    "CSV",
    "CLI",
    "bulk import"
  ],
  "author": "Matt Riley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/allcapsdev/cvcsv-cli/issues"
  },
  "homepage": "https://github.com/allcapsdev/cvcsv-cli#readme",
  "devDependencies": {
    "@babel/core": "7.21.0",
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "^17.0.0",
    "@evilmartians/lefthook": "1.3.3",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@tsconfig/recommended": "1.0.2",
    "@types/figlet": "1.5.5",
    "@types/jest": "28.1.7",
    "@types/node": "18.14.6",
    "@types/prompts": "2.4.2",
    "babel-loader": "9.1.2",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "jest": "26.6.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.4",
    "pretty-quick": "3.1.3",
    "rimraf": "4.2.0",
    "rollup": "2.79.1",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.34.1",
    "standard-version": "9.5.0",
    "ts-jest": "26.5.6",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "typedoc": "0.23.26",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "csv-writer": "1.6.0",
    "fast-glob": "3.2.12",
    "figlet": "1.5.2",
    "kleur": "4.1.5",
    "prompts": "2.4.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
