{
  "name": "@allcapsdev/cvcsv-cli",
  "version": "1.1.15",
  "description": "CLI for creating CSV for Cloud Vision Product Search API",
  "main": "./lib/index.js",
  "bin": {
    "cvcsv": "./bin/index.js"
  },
  "files": [
    "lib/**/*",
    "bin/**/*"
  ],
  "scripts": {
    "build": "npm run clean:some && webpack",
    "clean:lib": "rimraf ./lib",
    "clean:docs": "rimraf ./docs",
    "clean:node_modules": "rimraf ./node_modules",
    "clean:some": "run-p clean:lib clean:docs",
    "clean": "run-p prettier clean:lib clean:docs clean:node_modules",
    "commit": "git-cz",
    "commitlint": "commitlint -e",
    "release": "standard-version",
    "start:dev": "ts-node -T src/index.ts",
    "tscov": "tscov",
    "tscov:d": "tscov --details",
    "test": "jest",
    "posttest": "npm run size",
    "typedoc": "typedoc --out docs ./src",
    "prepare": "npm run build",
    "prettier": "prettier --parser typescript --write \"src/**/*.ts\"",
    "pretty-quick": "pretty-quick",
    "size": "size-limit",
    "get-version": "echo $npm_package_version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allcapsdev/cvcsv-cli.git"
  },
  "keywords": [
    "Google Cloud Vision Product Search",
    "CSV",
    "CLI",
    "bulk import"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/allcapsdev/cvcsv-cli/issues"
  },
  "homepage": "https://github.com/allcapsdev/cvcsv-cli#readme",
  "devDependencies": {
    "@arkweid/lefthook": "0.7.1",
    "@babel/core": "7.9.0",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@liftr/tscov": "1.4.5",
    "@size-limit/preset-small-lib": "4.4.0",
    "@types/figlet": "1.2.0",
    "@types/jest": "25.1.4",
    "@types/node": "13.9.8",
    "@types/prompts": "2.0.5",
    "babel-loader": "8.1.0",
    "commitizen": "4.0.3",
    "cz-conventional-changelog": "3.1.0",
    "jest": "25.2.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.2",
    "pretty-quick": "2.0.1",
    "rimraf": "3.0.2",
    "size-limit": "4.4.0",
    "standard-version": "7.1.0",
    "terser-webpack-plugin": "2.3.5",
    "ts-jest": "25.3.0",
    "ts-loader": "6.2.2",
    "ts-node": "8.8.1",
    "typedoc": "0.17.3",
    "typescript": "3.8.3",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3",
    "webpack-node-externals": "1.7.2"
  },
  "dependencies": {
    "csv-writer": "1.6.0",
    "figlet": "1.3.0",
    "globby": "11.0.0",
    "kleur": "3.0.3",
    "prompts": "2.3.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "size-limit": [
    {
      "limit": "9 KB",
      "config": "./webpack.config.js",
      "gzip": false
    }
  ],
  "standard-version": {
    "skip": {
      "tag": true
    }
  }
}
