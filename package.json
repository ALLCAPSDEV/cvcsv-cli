{
  "name": "@allcapsdev/cvcsv-cli",
  "version": "2.0.4",
  "description": "CLI for creating CSV for Cloud Vision Product Search API",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "cvcsv": "bin/index.js"
  },
  "files": [
    "lib/**/*",
    "bin/**/*"
  ],
  "scripts": {
    "build": "npm run clean:some && rollup -c",
    "clean:lib": "rimraf ./lib",
    "clean:docs": "rimraf ./docs",
    "clean:node_modules": "rimraf ./node_modules",
    "clean:some": "run-p clean:lib clean:docs",
    "clean": "run-p prettier clean:lib clean:docs clean:node_modules",
    "commit": "git-cz",
    "commitlint": "commitlint -e ./.git/COMMIT_EDITMSG",
    "release": "standard-version",
    "start:dev": "ts-node -T src/index.ts",
    "tscov": "tscov",
    "tscov:d": "tscov --details",
    "test": "jest",
    "watch": "jest --watch",
    "typedoc": "typedoc --out docs ./src",
    "prepare": "npm run build",
    "prettier": "prettier --parser typescript --write \"src/**/*.ts\"",
    "pretty-quick": "pretty-quick",
    "size": "size-limit",
    "get-version": "echo $npm_package_version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allcapsdev/cvcsv-cli.git"
  },
  "keywords": [
    "Google Cloud Vision Product Search",
    "CSV",
    "CLI",
    "bulk import"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/allcapsdev/cvcsv-cli/issues"
  },
  "homepage": "https://github.com/allcapsdev/cvcsv-cli#readme",
  "devDependencies": {
    "@arkweid/lefthook": "0.7.2",
    "@babel/core": "7.11.1",
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-conventional": "^10.0.0",
    "@liftr/tscov": "1.4.7",
    "@rollup/plugin-node-resolve": "9.0.0",
    "@tsconfig/recommended": "1.0.1",
    "@types/figlet": "1.2.0",
    "@types/jest": "26.0.10",
    "@types/node": "14.6.0",
    "@types/prompts": "2.0.8",
    "babel-loader": "8.1.0",
    "commitizen": "4.1.2",
    "cz-conventional-changelog": "3.2.0",
    "jest": "26.4.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1",
    "rimraf": "3.0.2",
    "rollup": "2.26.4",
    "rollup-plugin-size-snapshot": "0.12.0",
    "rollup-plugin-terser": "7.0.0",
    "rollup-plugin-typescript2": "0.27.2",
    "standard-version": "9.0.0",
    "ts-jest": "26.2.0",
    "ts-loader": "8.0.2",
    "ts-node": "8.10.2",
    "typedoc": "0.18.0",
    "typescript": "3.9.7"
  },
  "dependencies": {
    "csv-writer": "1.6.0",
    "figlet": "1.5.0",
    "globby": "11.0.1",
    "kleur": "4.1.1",
    "prompts": "2.3.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
