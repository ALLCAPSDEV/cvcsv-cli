{
  "name": "@matt-riley/cvcsv-cli",
  "version": "1.0.1",
  "description": "CLI for creating CSV for Cloud Vision Product Search API",
  "main": "./lib/index.js",
  "private": false,
  "bin": {
    "cvcsv": "./bin/index.js"
  },
  "files": [
    "lib/**/*",
    "bin/**/*"
  ],
  "scripts": {
    "build": "npm run clean:some && tsc -p .",
    "clean:lib": "rimraf ./lib",
    "clean:docs": "rimraf ./docs",
    "clean:node_modules": "rimraf ./node_modules",
    "clean:some": "run-p clean:lib clean:docs",
    "clean": "run-p prettier clean:lib clean:docs clean:node_modules",
    "start:dev": "ts-node -T src/index.ts",
    "tscov": "tscov",
    "tscov:d": "tscov --details",
    "test": "jest",
    "typedoc": "typedoc --out docs ./src",
    "prepare": "npm run build",
    "prettier": "prettier --parser typescript --write \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matt-riley/cvcsv-cli.git"
  },
  "keywords": ["Google Cloud Vision Product Search", "CSV", "CLI", "bulk import"],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matt-riley/cvcsv-cli/issues"
  },
  "homepage": "https://github.com/matt-riley/cvcsv-cli#readme",
  "devDependencies": {
    "@liftr/tscov": "1.4.3",
    "@types/figlet": "1.2.0",
    "@types/inquirer": "6.5.0",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.11",
    "jest": "24.9.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "rimraf": "3.0.0",
    "ts-jest": "24.1.0",
    "ts-node": "8.4.1",
    "typedoc": "0.15.0",
    "typescript": "3.6.3"
  },
  "dependencies": {
    "csv-writer": "1.5.0",
    "figlet": "1.2.4",
    "globby": "10.0.1",
    "inquirer": "7.0.0",
    "kleur": "3.0.3"
  }
}
