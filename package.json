{
  "name": "@allcapsdev/cvcsv-cli",
  "version": "1.1.11",
  "description": "CLI for creating CSV for Cloud Vision Product Search API",
  "main": "./lib/index.js",
  "private": false,
  "bin": {
    "cvcsv": "./bin/index.js"
  },
  "files": [
    "lib/**/*",
    "bin/**/*"
  ],
  "scripts": {
    "build": "npm run clean:some && webpack",
    "clean:lib": "rimraf ./lib",
    "clean:docs": "rimraf ./docs",
    "clean:node_modules": "rimraf ./node_modules",
    "clean:some": "run-p clean:lib clean:docs",
    "clean": "run-p prettier clean:lib clean:docs clean:node_modules",
    "commit": "git-cz",
    "commitlint": "commitlint -e",
    "release": "standard-version -s",
    "start:dev": "ts-node -T src/index.ts",
    "tscov": "tscov",
    "tscov:d": "tscov --details",
    "test": "jest",
    "posttest": "npm run size",
    "typedoc": "typedoc --out docs ./src",
    "prepare": "npm run build",
    "prettier": "prettier --parser typescript --write \"src/**/*.ts\"",
    "lint": "eslint 'src/**/*.ts'",
    "pretty-quick": "pretty-quick",
    "size": "size-limit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allcapsdev/cvcsv-cli.git"
  },
  "keywords": [
    "Google Cloud Vision Product Search",
    "CSV",
    "CLI",
    "bulk import"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/allcapsdev/cvcsv-cli/issues"
  },
  "homepage": "https://github.com/allcapsdev/cvcsv-cli#readme",
  "devDependencies": {
    "@arkweid/lefthook": "0.7.1",
    "@babel/core": "7.8.7",
    "@babel/preset-env": "7.8.7",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@liftr/tscov": "1.4.5",
    "@size-limit/preset-small-lib": "4.3.1",
    "@types/figlet": "1.2.0",
    "@types/jest": "25.1.4",
    "@types/node": "13.9.0",
    "@types/prompts": "2.0.4",
    "@typescript-eslint/eslint-plugin": "2.22.0",
    "@typescript-eslint/parser": "2.22.0",
    "babel-loader": "8.0.6",
    "commitizen": "4.0.3",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-prettier": "3.1.2",
    "jest": "25.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "rimraf": "3.0.2",
    "size-limit": "4.3.1",
    "standard-version": "7.1.0",
    "terser-webpack-plugin": "2.3.5",
    "ts-jest": "25.2.1",
    "ts-loader": "6.2.1",
    "ts-node": "8.6.2",
    "typedoc": "0.16.11",
    "typescript": "3.8.3",
    "webpack": "4.42.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3",
    "webpack-node-externals": "1.7.2"
  },
  "dependencies": {
    "@google-cloud/vision": "1.10.0",
    "csv-writer": "1.6.0",
    "figlet": "1.3.0",
    "globby": "11.0.0",
    "kleur": "3.0.3",
    "prompts": "2.3.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "standard-version": {
    "skip": {
      "tag": true
    }
  },
  "size-limit": [
    {
      "limit": "11 KB",
      "path": "./lib/index.js",
      "config": "./webpack.config.js",
      "gzip": false
    }
  ],
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "rules": {
      "@typescript-eslint/no-explicit-any": 0
    }
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2
  }
}
